

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model WeightLog {
  id     Int      @id @default(autoincrement())
  date   DateTime @default(now())
  weight Float
}

model MacroLog {
  id       Int      @id @default(autoincrement())
  date     DateTime @default(now())
  calories Int
  protein  Int
  carbs    Int
  fats     Int
}

model GitHubCommit {
  id      String   @id
  repo    String
  message String
  date    DateTime
}
